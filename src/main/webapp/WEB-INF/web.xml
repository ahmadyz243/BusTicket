<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <filter>
    <filter-name>f1</filter-name>
    <filter-class>filter.LoginFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>f1</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>

  <servlet>
    <servlet-name>HomePage</servlet-name>
    <servlet-class>view.HomePage</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>company-sign-up</servlet-name>
    <servlet-class>view.CompanySignUp</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>passenger-sign-up</servlet-name>
    <servlet-class>view.PassengerSignUp</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>company-menu</servlet-name>
    <servlet-class>view.CompanyMenu</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>passenger-menu</servlet-name>
    <servlet-class>view.PassengerMenu</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>buy-ticket</servlet-name>
    <servlet-class>view.BuyTicket</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>view-tickets</servlet-name>
    <servlet-class>view.ViewTickets</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>delete-ticket</servlet-name>
    <servlet-class>view.DeleteTicket</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>delete-ticket</servlet-name>
    <url-pattern>/delete-ticket</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>view-tickets</servlet-name>
    <url-pattern>/view-tickets</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>buy-ticket</servlet-name>
    <url-pattern>/buyticket</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>passenger-menu</servlet-name>
    <url-pattern>/passengermenu</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>company-menu</servlet-name>
    <url-pattern>/companymenu</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>passenger-sign-up</servlet-name>
    <url-pattern>/passengersignup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>company-sign-up</servlet-name>
    <url-pattern>/companysignup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HomePage</servlet-name>
    <url-pattern>/HomePage</url-pattern>
  </servlet-mapping>
</web-app>
